

pipeline {

	agent any 
	stages {
		
		stage ("build_job") {
			
			steps {
				
				sh "ansible devlinux -b -m yum -a 'pkg=httpd state=present'"
				sh "ansible devlinux -b -m service -a 'name=httpd state=started'"
				sh "ansible devlinux -b -m copy -a 'src=/mnt/project/ansible-pipeline/23q2/index.html dest=/var/www/html/'"
				

			}

		}
	}
} 
